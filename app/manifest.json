{
  "manifest_version": 2,
  "name": "Force Autocomplete",
  "version": "1.1",
  "author": "NUMEDIAWEB",
  "content_scripts": [{
    "js": ["assets/js/jquery-2.1.3.min.js", "assets/js/jquery.nw.force_autocomplete.js", "app.js"],
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_start"
  }],
  "default_locale": "en",
  "description": "Force autocomplete attribute in input fields to be on. Allows website form fields to remember what you typed the next time you re-visit them.",
  "icons": {
    "16": "assets/images/icon16.png",
    "32": "assets/images/icon32.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  }
}